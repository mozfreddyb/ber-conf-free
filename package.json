{
  "name": "ber-conf-free",
  "version": "0.1.0",
  "author": "Frederik Braun <fbraun@mozilla.com>",
  "description": "A Node server that gathers the free/busy calendar information for conference rooms in Mozilla Berlin",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "supervisor server.js",
    "push": "git push heroku master",
    "outdated": "npm outdated --depth=0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozfreddyb/ber-conf-free.git"
  },
  "keywords": [
    "yvr",
    "conference",
    "room"
  ],
  "dependencies": {
    "config": "~1.12.0",
    "ejs": "~2.3.1",
    "express": "~4.12.0",
    "conf-free-api": "git+https://github.com/mozfreddyb/conf-free-api.git",
    "lodash": "~3.6.0",
    "moment": "~2.10.0",
    "react": "~0.13.0"
  },
  "license": "MPL",
  "engines": {
    "node": "0.10.x"
  },
  "devDependencies": {
    "browserify": "latest",
    "del": "~1.1.1",
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-concat": "latest",
    "gulp-jshint": "latest",
    "gulp-less": "latest",
    "gulp-license": "latest",
    "gulp-livereload": "latest",
    "gulp-load-plugins": "latest",
    "gulp-notify": "latest",
    "gulp-react": "latest",
    "gulp-size": "latest",
    "gulp-util": "latest",
    "literalify": "latest",
    "node-static": "latest",
    "reactify": "latest",
    "vinyl-source-stream": "latest",
    "watchify": "latest"
  },
  "paths": {
    "less": "./styles/main.less",
    "js": "./app/*.js",
    "jsx": "./app/components/*.jsx",
    "app": "./app/main.js",
    "html": "./app/index.html"
  },
  "dest": {
    "style": "css/main.css",
    "app": "js/main.js",
    "vendorjs": "js/vendor.js",
    "fonts": "public/fonts",
    "dist": "public"
  }
}
