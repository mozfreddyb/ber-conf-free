{
  "name": "ber-conf-free",
  "version": "0.1.0",
  "author": "Frederik Braun <fbraun@mozilla.com>",
  "description": "A Node server that gathers the free/busy calendar information for conference rooms in Mozilla Berlin",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "supervisor server.js",
    "push": "git push heroku master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozfreddyb/ber-conf-free.git"
  },
  "keywords": [
    "yvr",
    "conference",
    "room"
  ],
  "dependencies": {
    "config": "~1.10.0",
    "ejs": "~1.0.0",
    "express": "~4.8.0",
    "gcal-conf-free-api": "~0.3.0",
    "lodash": "~3.1.0",
    "moment": "~2.9.0",
    "react": "~0.12.2"
  },
  "license": "MPL",
  "engines": {
    "node": "0.10.x"
  },
  "devDependencies": {
    "browserify": "~8.1.1",
    "del": "~1.1.1",
    "gulp": "~3.8.10",
    "gulp-autoprefixer": "~2.1.0",
    "gulp-concat": "~2.2.0",
    "gulp-jshint": "~1.9.0",
    "gulp-less": "~2.0.1",
    "gulp-license": "~1.0.0",
    "gulp-livereload": "~3.4.0",
    "gulp-load-plugins": "~0.8.0",
    "gulp-notify": "~2.1.0",
    "gulp-react": "~2.0.0",
    "gulp-size": "~1.2.0",
    "gulp-util": "~3.0.2",
    "literalify": "~0.3.0",
    "node-static": "~0.7.6",
    "reactify": "~1.0.0",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "~2.2.1"
  },
  "paths": {
    "less": "./styles/main.less",
    "js": "./app/*.js",
    "jsx": "./app/components/*.jsx",
    "app": "./app/main.js",
    "html": "./app/index.html"
  },
  "dest": {
    "style": "css/main.css",
    "app": "js/main.js",
    "vendorjs": "js/vendor.js",
    "fonts": "public/fonts",
    "dist": "public"
  }
}
